{
    "name": "tsaiyihua/laravel-pagecache",
    "description": "Page Cache for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "cache", "page", "pagecache"],
    "require": {
        "laravel/framework": "^7|^8.75.0|^9",
        "guzzlehttp/guzzle": "^7.2",
        "ext-redis": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "orchestra/testbench": "^7.16"
    },
    "authors": [
        {
            "name": "Yihua Tsai",
            "email": "tsaiyihua@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TsaiYiHua\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TsaiYiHua\\Cache\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TsaiYiHua\\Cache\\CacheServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
